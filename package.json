{
  "name": "dok-soon-ee",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:prod": "next dev -p 3000",
    "build": "next build",
    "build:dev": "env-cmd -f .env.development next build",
    "build:prod": "env-cmd -f .env.production next build",
    "start": "next start -H 0.0.0.0",
    "start:dev": "env-cmd -f .env.development next start -H 0.0.0.0",
    "start:prod": "env-cmd -f .env.production next start -H 0.0.0.0",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.884.0",
    "@aws-sdk/s3-presigned-post": "^3.884.0",
    "bcryptjs": "^3.0.2",
    "browser-image-compression": "^2.0.2",
    "file-saver": "^2.0.5",
    "jose": "^6.0.13",
    "mssql": "^11.0.1",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "sass": "^1.90.0",
    "tailwindcss": "^4"
  }
}
